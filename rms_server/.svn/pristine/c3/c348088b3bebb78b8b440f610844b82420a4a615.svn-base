package kr.or.ddit.rms.vo;

import java.io.Serializable;

public class Board_classVO implements Serializable{
	
	String bc_num;
	String bc_name;
	String bc_idx;
	
	public String getBc_idx() {
		return bc_idx;
	}
	public void setBc_idx(String bc_idx) {
		this.bc_idx = bc_idx;
	}
	public String getBc_num() {
		return bc_num;
	}
	public void setBc_num(String bc_num) {
		this.bc_num = bc_num;
	}
	public String getBc_name() {
		return bc_name;
	}
	public void setBc_name(String bc_name) {
		this.bc_name = bc_name;
	}
	
}
