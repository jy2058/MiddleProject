package kr.or.ddit.rms.vo;

import java.io.Serializable;

public class ShelterVO implements Serializable {
	String shel_id;//보호기관ID
	String shel_name;//보호기관이름
	String shel_loc;//보호기관위치
	String shel_tel;//보호기관연락처
	String shel_email;//보호기관이메일
	String shel_bsnum; // 보호기관 사업자번호
	
	public String getShel_bsnum() {
		return shel_bsnum;
	}
	public void setShel_bsnum(String shel_bsnum) {
		this.shel_bsnum = shel_bsnum;
	}
	public String getShel_id() {
		return shel_id;
	}
	public void setShel_id(String shel_id) {
		this.shel_id = shel_id;
	}
	public String getShel_name() {
		return shel_name;
	}
	public void setShel_name(String shel_name) {
		this.shel_name = shel_name;
	}
	public String getShel_loc() {
		return shel_loc;
	}
	public void setShel_loc(String shel_loc) {
		this.shel_loc = shel_loc;
	}
	public String getShel_tel() {
		return shel_tel;
	}
	public void setShel_tel(String shel_tel) {
		this.shel_tel = shel_tel;
	}
	public String getShel_email() {
		return shel_email;
	}
	public void setShel_email(String shel_email) {
		this.shel_email = shel_email;
	}


}
